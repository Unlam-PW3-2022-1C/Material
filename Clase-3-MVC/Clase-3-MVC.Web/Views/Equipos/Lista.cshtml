@model List<EquipoViewModel>
@{
    ViewData["Title"] = "Equipos";
}


<div class="contenedor-tabla">
    <table class="table table-dark">
        <thead>
            <tr>
                <th scope="col">Nombre</th>
                <th scope="col">Pais</th>
                <th scope="col">Editar</th>
                @*<th scope="col">Eliminar</th>*@
            </tr>
        </thead>
        <tbody>
            @foreach (var equipo in Model)
            {
                <tr>
                    <td>@equipo.Nombre</td>
                    <td>@equipo.Pais</td>
                    <td><a href="/Equipos/editar/@equipo.Id">Editar</a></td>
                    <td>@*<a href="/Equipos/eliminar/@equipo.Id">Eliminar</a>*@</td>
                </tr>
            }
        </tbody>
    </table>

</div>


